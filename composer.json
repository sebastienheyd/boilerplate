        "hieu-le/active": "^3.4",
        "yajra/laravel-datatables-oracle": "^8.0",
        "lavary/laravel-menu": "^1.6",
        "jenssegers/date": "^3.2",
        "intervention/image": "^2.3",
        "arcanedev/log-viewer": "^4.3"
