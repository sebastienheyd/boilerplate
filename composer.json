{
    "name": "sebastienheyd/boilerplate",
    "description": "Laravel Boilerplate based on AdminLTE 3 with user management, roles, permissions, logs viewer, ...",
    "keywords": ["laravel", "boilerplate", "backend","adminlte", "laratrust", "laravelcollective", "active", "admin"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^6.0|^7.0",
        "laravelcollective/html": "^6.0|^7.0",
        "santigarcor/laratrust": "^6.0",
        "hieu-le/active": "^4.0|^7.0",
        "yajra/laravel-datatables-oracle": "^9.0",
        "lavary/laravel-menu": "^1.7",
        "intervention/image": "^2.5",
        "arcanedev/log-viewer": "^5.0|^7.0",
        "nesbot/carbon": "^2.0",
        "thomaswelton/laravel-gravatar": "^1.2",
        "laravel/ui": "^1.2|^2.0",
        "caouecs/laravel-lang": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^5.0",
        "doctrine/dbal": "^2.9"
    },
    "suggest": {
        "sebastienheyd/boilerplate-media-manager": "Media manager",
        "sebastienheyd/boilerplate-email-editor": "Visual editor for e-mails"
    },
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "SÃ©bastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
